{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "max-len": "off"
  }
}